    "typescript": "4.9.5",
    "typescript-eslint": "^0.0.1-alpha.0",
    "wait-on": "^5.1.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@emotion/is-prop-valid": "^1.3.1",
    "@nulogy/tokens": "^6.1.1",
    "@radix-ui/react-tooltip": "1.0.7",
    "@reach/dialog": "0.17.0",
    "@styled-system/prop-types": "^5.1.4",
    "@styled-system/theme-get": "^5.1.2",
    "@types/react-window": "^1.8.8",
    "@types/styled-system": "5.1.22",
    "core-js": "3",
    "date-fns": "2.23.0",
    "debounce": "^1.2.0",
    "deep-equal": "^2.2.1",
    "framer-motion": "6.5.1",
    "i18next": "^19.3.1",
    "polished": "3.4.4",
    "react-datepicker": "^4.25.0",
    "react-fast-compare": "^3.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^12.3.1",
    "react-modal": "^3.14.4",
    "react-popper": "1.3.11",
    "react-popper-2": "npm:react-popper@2.2.4",
    "react-resize-detector": "^9.1.0",
    "react-select": "^5.9.0",
    "react-window": "^1.8.11",
    "smoothscroll-polyfill": "^0.4.4",
    "styled-system": "^5.1.4",
    "stylis": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run check"
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.js?$": "babel-jest",
      "^.+\\.ts?$": "babel-jest",
      "^.+\\.tsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@nulogy|storybook-addon-performance))"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress"
    ],
    "testMatch": [
      "**/*.spec.tsx",
      "**/*.spec.ts"
    ],
    "setupFiles": [
      "<rootDir>/spec/support/registerContext.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/spec/support/specHelper.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
